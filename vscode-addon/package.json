{
	"name": "becoder",
	"publisher": "become",
	"displayName": "becoder",
	"description": "Be a coder? cool!",
	"version": "0.0.4",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.openMainUI",
		"onCommand:extension.runScript",
		"onCommand:extension.uploadScript",
		"*:extension.openMainUI"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.openMainUI",
				"title": "becoder"
			},
			{
				"command": "extension.runScript",
				"title": "run in WiFi block"
			},
			{
				"command": "extension.uploadScript",
				"title": "upload in WiFi block"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.openMainUI",
					"group": "navigation"
				},
				{
					"command": "extension.runScript",
					"group": "navigation"
				},
				{
					"command": "extension.uploadScript",
					"group": "navigation"
				}
			],
			"editor/context": [
				{
					"command": "extension.openMainUI",
					"group": "navigation"
				},
				{
					"command": "extension.runScript",
					"group": "navigation"
				},
				{
					"command": "extension.uploadScript",
					"group": "navigation"
				}
			],
			"editor/title/context": [
				{
					"command": "extension.openMainUI",
					"group": "navigation"
				},
				{
					"command": "extension.runScript",
					"group": "navigation"
				},
				{
					"command": "extension.uploadScript",
					"group": "navigation"
				}
			],
			"editor/title": [
				{
					"command": "extension.openMainUI",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^10.2.0",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0"
	},
	"dependencies": {
		"@types/serialport": "^7.0.4",
		"regenerator": "^0.14.2",
		"serialport": "^8.0.5",
		"uglify-js": "^3.7.2"
	},
	"repository": "git@code.aliyun.com:become/becoder.git"
}
